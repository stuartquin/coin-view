{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "jsx-a11y/anchor-is-valid": "off",
        "react/forbid-prop-types": "off",
        "react/jsx-filename-extension": "off",
        "space-before-function-paren": "off",
        "function-paren-newline": "off",
        "comma-dangle": "off",
        "quotes": "off",
        "no-confusing-arrow": "off",
        "arrow-body-style": "off",
        "arrow-parens": "off"
    }
}
